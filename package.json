  "bugs": {
    "url": "https://github.com/bradtraversy/expense-tracker-react/issues"
  },
  "homepage": "https://github.com/bradtraversy/expense-tracker-react#readme",
  "dependencies": {
    "colors": "^1.4.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.9.14",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "nodemon": "^2.0.3"
  }
}
